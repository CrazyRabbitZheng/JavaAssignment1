package org.example;

import java.security.SecureRandom;

public class RandomIntegers {
    public static void main(String[] args) {
        SecureRandom randomInts = new SecureRandom();

        for (int i = 1; i <= 20; i++) {
            int face = 1 + randomInts.nextInt(6);
            System.out.print(face + " ");

            if(i % 5 == 0){
                System.out.println();
            }
            
        }
    }
}
